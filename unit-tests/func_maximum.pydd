# Intrepydd code to test maximum function.
# Author: Sitao Huang (shuang91@illinois.edu)

def maximum_test(a: Array(float64, 2), b: Array(float64, 2)) -> Array(float64, 2):
    return maximum(a, b)

def maximum_test_2(a: Array(float64, 2), b: Array(int64, 2)) -> Array(float64, 2):
    return maximum(a, b)

def maximum_test_scalar(a: Array(float64, 2), b: int32) -> Array(float64, 2):
    return maximum(a, b)

def maximum_test_scalar_2(a: int32, b: Array(float64, 2)) -> Array(float64, 2):
    return maximum(a, b)
